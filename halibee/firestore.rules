rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /freelancers/{freelancer} {
      allow read: if true; 
      allow write: if request.auth != null && request.auth.uid == freelancer;
    }
    match /categories/{skill} {
      allow read: if true; 
      allow write: if false;
    }
  }
}